{
    "name": "sdon2/laravel-audit-log",
    "description": "A very simple audit logger to monitor the users of your website or application",
    "homepage": "https://github.com/sdon2/laravel-audit-log",
    "keywords": [
        "log",
        "user",
        "activity",
        "audit",
        "laravel"
    ],
    "authors": [
        {
            "name": "Saravanakumar Arumugam",
            "email": "saravanakumar.a.o@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
        "illuminate/config": "^6.0|^7.0|^8.0",
        "illuminate/database": "^6.0|^7.0|^8.0",
        "illuminate/support": "^6.0|^7.0|^8.0"
    },
    "require-dev": {
        "ext-json": "*",
        "orchestra/testbench": "^4.0|^5.0",
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Sdon2\\AuditLog\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sdon2\\AuditLog\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sdon2\\AuditLog\\AuditLogServiceProvider"
            ]
        }
    },
    "license": "MIT"
}
